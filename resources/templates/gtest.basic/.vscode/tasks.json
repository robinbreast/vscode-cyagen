{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build Google Test",
      "type": "shell",
      "command": "cmake",
      "args": [
        "-S",
        "${workspaceFolder}",
        "-B",
        "${workspaceFolder}/build",
        "&&",
        "cmake",
        "--build",
        "${workspaceFolder}/build"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
